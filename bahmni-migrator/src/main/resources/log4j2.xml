<?xml version="1.0" encoding="UTF-8"?>
<!--
############################################################################
##
##  This is the log configuration for production.
##
############################################################################
-->
<Configuration status="warn">

    <Appenders>
    
        
        <Console name="console">
            <PatternLayout pattern="%d %-5p [%c{1}] %m%n"/>
        </Console>
        
    </Appenders>
    
    <Loggers>
        
        <Root level="INFO">
            <AppenderRef ref="console"/>
        </Root>      
    
    </Loggers>
    
</Configuration>
